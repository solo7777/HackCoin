# syncToGitHub.ps1
     # Change to project directory
     Set-Location -Path "D:\HackCoin"
     # Fetch and merge changes from remote repository
     git pull origin main
     # Stage all changes
     git add .
     # Create commit with timestamp
     $commitMessage = "Project update: $(Get-Date -Format 'yyyy-MM-dd HH:mm:ss')"
     git commit -m $commitMessage
     # Push changes to GitHub
     git push origin main
     # Output confirmation
     Write-Output "Changes synchronized with GitHub"